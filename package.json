{
  "name": "knex-mocker",
  "version": "1.0.5",
  "description": "Mocks knex for testing purposes. Copies database tables from existing schema into a testing schema and allows the table to be seeded. Setup and teardown happens by making function calls. Currently only tested on mysql. ",
  "main": "dist/index.js",
  "scripts": {
    "start": "nodemon src/index.js  --exec babel-node",
    "build": "node_modules/.bin/babel src -d dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/averyduffin/knex-mocker.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.18.1",
    "nodemon": "^1.12.5",
    "prettier": "1.10.2",
    "pretty-quick": "^1.4.1"
  },
  "dependencies": {
    "async": "^2.6.0",
    "fs": "0.0.1-security",
    "knex": "^0.14.4",
    "moment": "^2.22.0",
    "mysql": "^2.15.0",
    "pg": "^7.4.1"
  },
  "keywords": [
    "knex",
    "knex-mocker",
    "testing",
    "test",
    "mock"
  ],
  "author": "Avery Duffin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/averyduffin/knex-mocker/issues"
  },
  "homepage": "https://github.com/averyduffin/knex-mocker#readme"
}
